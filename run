#!/bin/sh

export NODE_PATH="${HOME}/node_modules"
MONGO_URL="${DOTCLOUD_DB_MONGODB_LOGIN}:${DOTCLOUD_DB_MONGODB_PASSWORD}@${DOTCLOUD_DB_MONGODB_HOST}:${DOTCLOUD_DB_MONGODB_PORT}"

exec ~/cactusdb.exe \
         --logs-path /var/log/supervisor \
         --http-port $PORT_WWW \
         $@
